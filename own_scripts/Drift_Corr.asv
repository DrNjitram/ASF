%%create 2 datasets for different frames
M1 = randn(100, 8);
M2 = [M1(:,1), M1(:,2)];
M0 = ones(100,2);
M3 = M2+M0;

MT1 = [M2(:,:),ones(100,1)]; %x and y, frame 1
MT2 = [M3(:,:),ones(100,1)*2]; %x+1 and y+1, frame 2
%%
F1 = [MT1(:,1)];
F2 = [MT2(:,1)];
delta = F2-F1;
M = [median(delta)*ones(length(F2),];

MT2 = MT2-M;