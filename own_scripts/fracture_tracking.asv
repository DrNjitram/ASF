clear 


fname = 'data\colloids.tif';
info = imfinfo(fname);
num_images = numel(info);

images = readtiffstack(fname);
r = {num_images}
for i = 1:num_images
    disp(i)
    r{i} = feature2D(images(:, :, i),1,3,3000);
end



no = 2;
imagex = images(:, :, 2);
rx = r{2};

figure
imshow(imagex, [0, max(max(imagex))]);
hold on
plot(rx(:,1), rx(:,2), '.');

figure
histogram(mod(rx(:,1), 1), 10)
figure
histogram(mod(rx(:,2), 1), 10)